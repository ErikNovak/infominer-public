<!-- Header -->
<thead>
  <tr>
    <th>
    </th>
    {{#each fields as |field|}}
      <th class="{{field.name}}">{{field.name}}</th>
    {{/each}}
  </tr>
</thead>
<!-- Body -->
<tbody>
  {{#each documents as |document| }}
    {{ document-table-row document=document fields=fields }}
    {{ document-table-row-information document=document fields=fields }}
  {{/each}}
</tbody>