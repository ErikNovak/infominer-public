{
  "name": "infominer",
  "version": "1.0.0",
  "description": "OntoGen++",
  "main": "index.js",
  "scripts": {
    "test": "cd src/test && sh tests.sh",
    "preinstall": "npm list ember-cli@2.16.2 -g || npm install ember-cli@2.16.2 -g",
    "postinstall": "cd src/server/ember-application && npm install",
    "ember:serve": "cd src/server/ember-application && ember serve",
    "ember:build": "cd src/server/ember-application && ember build --output-path=../public --environment=production",
    "server:gui": "cd src/server && node gui-server.js",
    "server:guiDev": "cd src/server && node gui-server.js --ignoreSecurity",
    "server:guiDebug": "cd src/server && node debug gui-server.js",
    "postgres:create": "cd src/load && node create_postgres_tables.js"
  },
  "config": {
    "portGui": 3000
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErikNovak/InfoMiner.git"
  },
  "author": "Erik Novak <erik.novak@ijs.si>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/ErikNovak/InfoMiner/issues"
  },
  "homepage": "https://github.com/ErikNovak/InfoMiner#readme",
  "dependencies": {
    "async": "^2.6.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "jsonschema": "^1.2.2",
    "minimist": "^1.2.0",
    "multer": "^1.3.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-saml": "^0.32.1",
    "passport-twitter": "^1.0.4",
    "path": "^0.12.7",
    "pg": "^7.4.0",
    "pg-cursor": "^1.3.0",
    "qminer": "^9.1.2",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2"
  },
  "devDependencies": {
    "eslint": "^4.10.0"
  }
}
